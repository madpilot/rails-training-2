<h1>Prices for <%= @stock.code %></h1>
<table>
  <thead>
    <tr>
      <th>Date</th><th>Open</th><th>Close</th><th>Low</th><th>High</th><th>Value</th><th>Edit</th>
    </tr>
  </thead>
  <tbody>
    <% @prices.each do |price| -%>
      <tr>
        <td><%= price.date %></td>
        <td><%= price.open_price %></td>
        <td><%= price.close_price %></td>
        <td><%= price.low_price %></td>
        <td><%= price.high_price %></td>
        <td><%= price.value %></td>
        <td><%= link_to 'Edit', edit_stock_price_path(@stock, price) %></td>
      </tr>
    <% end -%>
  </tbody>
</table>
